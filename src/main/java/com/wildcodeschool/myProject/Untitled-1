@RequestMapping("/doctor1")
	@ResponseBody
	public String doctor1 (){
		return "William Hartnell";		
	}
	@RequestMapping("/doctor4")
	@ResponseBody
	public String doctor4 (){
		return "Tom Bakert";
	}
